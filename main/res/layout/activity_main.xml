<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/Menu"
        android:layout_width="32dp"
        android:layout_height="37dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="44dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/category" />

    <ImageView
        android:id="@+id/Icon"
        android:layout_width="82dp"
        android:layout_height="40dp"
        android:layout_marginLeft="164dp"
        android:layout_marginTop="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_6" />

    <ImageView
        android:id="@+id/Alarm"
        android:layout_width="38dp"
        android:layout_height="31dp"
        android:layout_marginLeft="356dp"
        android:layout_marginTop="48dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/notification" />

    <View
        android:id="@+id/BackGround"
        android:layout_width="413dp"
        android:layout_height="355dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:alpha="0.66"
        android:background="@drawable/rectangle_4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

    <ImageView
        android:id="@+id/Search_Bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="152dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/boxes" />

    <ImageView
        android:id="@+id/Search_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="328dp"
        android:layout_marginTop="296dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search" />

    <TextView
        android:id="@+id/Start"
        android:layout_width="219dp"
        android:layout_height="61dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="152dp"
        android:text="출발"
        android:textAlignment="textStart"
        android:textColor="#C8C8C8"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/start2"
        android:layout_width="219dp"
        android:layout_height="61dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="204dp"
        android:text="도착"
        android:textAlignment="textStart"
        android:textColor="#C8C8C8"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="89dp"
        android:layout_height="41dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="384dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/category_bar" />

    <TextView
        android:id="@+id/popular"
        android:layout_width="61dp"
        android:layout_height="23dp"
        android:gravity="top"
        android:text="인기있는"
        android:textColor="#FEFEFE"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="392dp" />

    <TextView
        android:id="@+id/popular2"
        android:layout_width="37dp"
        android:layout_height="25dp"
        android:gravity="top"
        android:text="좋은"
        android:textColor="#0E0E0E"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="391dp" />

    <TextView
        android:id="@+id/popular3"
        android:layout_width="48dp"
        android:layout_height="24dp"
        android:gravity="top"
        android:text="가까운"
        android:textColor="#0E0E0E"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="392dp" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/category_2"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="385dp" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/category_2"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="385dp" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/category_2"
        tools:layout_editor_absoluteX="344dp"
        tools:layout_editor_absoluteY="385dp" />


</androidx.constraintlayout.widget.ConstraintLayout>